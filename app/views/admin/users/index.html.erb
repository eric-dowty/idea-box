<h1>Welcome <%= current_user.name %> </h1>

<% @users.each do |user| %>
  <h3><%= user.username %></h3>
  <ul class="nav nav-pills", id="<%=dom_id(user)%>">
    <li><%= link_to "Show", admin_user_path(user), method: :get, class: "btn btn-info", id: "btn-sm"%></li>
    <li><%= link_to "Edit", edit_admin_user_path(user), method: :get, class: "btn btn-info", id: "btn-sm" %></li>
    <li><%= button_to "Delete", admin_user_path(user), method: :delete, class: "btn btn-danger", id: "btn-sm", data: {confirm: "Really?"} %></li>
  </ul>
<% end %>